{
  "name": "timeline",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/timeline/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/timeline",
        "index": "apps/timeline/src/index.html",
        "baseHref": "/",
        "main": "apps/timeline/src/main.tsx",
        "polyfills": "apps/timeline/src/polyfills.ts",
        "tsConfig": "apps/timeline/tsconfig.app.json",
        "assets": ["apps/timeline/src/favicon.ico", "apps/timeline/src/assets"],
        "styles": [
          "apps/timeline/src/font-awesome/styles.css",
          "https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap"
        ],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "timeline:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "timeline:build:development"
        },
        "production": {
          "buildTarget": "timeline:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/timeline/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/timeline"],
      "options": {
        "jestConfig": "apps/timeline/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
